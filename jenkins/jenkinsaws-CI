pipeline {
    agent any
    stages {
        stage('compile') {
	   steps {
                echo 'compiling..'
		git url: 'https://github.com/lerndevops/samplejavaapp'
		sh script: '/opt/maven/bin/mvn compile'
           }
        }
	 stage('qa-matrics-check') {
	   steps {
                echo 'validate......'
		sh script: '/opt/maven/bin/mvn validate'	
           }		
        }
        stage('package') {
	   steps {
                echo 'package......'
		sh script: '/opt/maven/bin/mvn package'	
           }		
        }
    }
}
